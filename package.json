{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "description": "Vue.js example starter project",
  "scripts": {
    "serve": "vue-cli-service serve --mode development --port 3000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "http-server": "cd ./dist && http-server --gzip -c31536000",
    "serve-prod": "SET NODE_OPTIONS=--openssl-legacy-provider && workbox generateSW workbox-config.js && vue-cli-service serve --mode production"
  },
  "dependencies": {
    "autoprefixer": "^10.4",
    "core-js": "^3.8.3",
    "express": "^4.18.1",
    "postcss": "^8.4",
    "spdy": "^4.0.2",
    "tailwindcss": "^3.2",
    "vue": "^3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-eslint": "^10.1",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "prettier": "^2.8",
    "sharp": "^0.31.3",
    "typescript": "^4.9",
    "vue-cli-plugin-tailwind": "^3.0.0",
    "@vue/compiler-sfc": "^3.2" ,
    "workbox-cli": "^6.5.4"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "starter"
  ]
}
